{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "OptionsDebugFeature": {
    "Enabled": true,
    "RootPath": "/debug/options",
    "IncludeInHealthChecks": true
  },

  "ServicesDebugFeature": {
    "Enabled": true,
    "RootPath": "/debug/services",
    "IncludeInHealthChecks": true
  },

  "HostedServicesDebugFeature": {
    "Enabled": true,
    "RootPath": "/debug/hosted"
  },

  "HealthChecksDebugFeature": {
    "Enabled": true,
    "RootPath": "/debug/health"
  },

  "EnvironmentDebugFeature": {
    "Enabled": true,
    "RootPath": "/env"
  },

  "FeaturesDebugFeature": {
    "Enabled": true,
    "RootPath": "/debug/features"
  },

  "RoutesDebugFeature": {
    "Enabled": true,
    "RootPath": "/debug/routes"
  },

  "CachesDebugFeature": {
    "Enabled": true,
    "RootPath": "/debug/caches"
  }
}
